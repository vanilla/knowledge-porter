{
    "name": "vanilla/knowledge-porter",
    "description": "A command line porter for Vanilla Knowledge",
    "type": "project",
    "authors": [
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "vanilla/garden-cli": "^4.0",
        "vanilla/garden-container": "*",
        "vanilla/garden-schema": "~1.10.2",
        "vanilla/garden-http": "^2.1",
        "psr/log": "^3.0",
        "psr/simple-cache": "*",
        "symfony/cache" : "^6.2",
        "ext-dom": "*",
        "ext-mbstring": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "vanilla/standards": "^1.3",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Vanilla\\KnowledgePorter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilla\\KnowledgePorter\\Tests\\": "tests"
        }
    },
    "bin": [
        "bin/knowledge-porter"
    ]
}
